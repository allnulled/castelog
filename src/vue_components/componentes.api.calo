                        si con_propagacion hago this.alCambiarFecha(nueva_fecha, this).
                        hago this.$forceUpdate(true).
                        retorno 1.
                    }.
                    si dia es tipo objeto {
                        si dia.dia no es tipo número normal lanzo un nuevo Error("Required parameter «dia.dia» to be number in order to «Calendario.seleccionar_dia»").
                        creo nueva_fecha como un nuevo Date(this.internal_value_date).
