        { return `await Castelog.metodos.analizo_sistema(${parametros});` }
    Subsentencia_con_parametros_2 = _* "con par√°metros como" _+
      generativa:Generativa
        { return `Castelog.metodos.analizo_sistema(${generativa});` }
} con prescript como {
